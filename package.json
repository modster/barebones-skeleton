{
  "name": "barebones-skeleton",
  "version": "0.1.1",
  "license": "MIT",
  "description": "W.I.P.",
  "private": true,
  "type": "module",
  "main": "index.js",
  "homepage": "https://datadash.cloud",
  "repository": {
    "type": "git",
    "url": "git+"
  },
  "bugs": {
    "url": "https://github.com/modster/barebones-skeleton/issues",
    "email": "support@datadash.cloud"
  },
  "author": "@emgreeff",
  "contributors": [
    "@modster",
    "@skeleton"
  ],
  "keywords": [
    "Skeleton",
    "SvelteKit",
    "Threlte",
    "Theatrejs",
    "Tailwindcss",
    "Mdx",
    "Storybook"
  ],
  "categories": [
    "Formatters",
    "Linters",
    "Snippets",
    "Testing",
    "Themes",
    "Visualization",
    "Other"
  ],
  "scripts": {
    "dev": "vite dev --open",
    "host": "vite dev --open --host",
    "build": "vite build",
    "preview": "wrangler pages dev .svelte-kit/cloudflare",
    "lint": "prettier . --check .",
    "format": "prettier . --write .",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "tailwind": "npx tailwindcss -i ./src/app.postcss -o ./.svelte-kit/cloudflare/global.css",
    "tailwind:watch": "npx tailwindcss -i ./src/app.postcss -o ./.svelte-kit/cloudflare/global.css --watch",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "2.0.0",
    "@skeletonlabs/tw-plugin": "0.1.0",
    "@storybook/addon-essentials": "^7.4.1",
    "@storybook/addon-interactions": "^7.4.1",
    "@storybook/addon-links": "^7.4.1",
    "@storybook/blocks": "^7.4.1",
    "@storybook/svelte": "^7.4.1",
    "@storybook/sveltekit": "^7.4.1",
    "@storybook/testing-library": "^0.2.0",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.3.3",
    "@sveltejs/kit": "^1.20.4",
    "@tailwindcss/forms": "0.5.6",
    "@tailwindcss/typography": "0.5.10",
    "autoprefixer": "10.4.15",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "postcss": "8.4.29",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "storybook": "^7.4.1",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "tailwindcss": "3.3.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-tailwind-purgecss": "0.1.3"
  },
  "dependencies": {
    "@dimforge/rapier3d-compat": "^0.11.2",
    "@floating-ui/dom": "1.5.1",
    "@theatre/core": "^0.7.0",
    "@theatre/studio": "^0.7.0",
    "@threlte/core": "^6.0.8",
    "@threlte/extras": "^5.6.2",
    "@threlte/rapier": "^1.1.2",
    "@threlte/theatre": "^2.1.2",
    "@threlte/xr": "^0.0.4",
    "@types/three": "^0.156.0",
    "highlight.js": "11.8.0",
    "simplex-noise": "^4.0.1",
    "three": "^0.156.1"
  },
  "lint-staged": {
    "*.{js,ts,css,md,mdx,json,svelte}": "prettier . --write"
  }
}
